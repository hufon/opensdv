CREATE TABLE ARTICLE (ID VARCHAR(255) NOT NULL, DESIGNATION VARCHAR(255), INITIALPRICE INTEGER, MINIMUMPRICE INTEGER, RETIRED BOOLEAN, SELLINGPRICE INTEGER, VENDOR_ID VARCHAR(255), VENTE_ID VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE VENTE (ID VARCHAR(255) NOT NULL, DATE TIMESTAMP, LOCATION VARCHAR(255), NAME VARCHAR(255), YEAR INTEGER, PRIMARY KEY (ID))
CREATE TABLE VENDOR (ID VARCHAR(255) NOT NULL, NAME VARCHAR(255), RATE INTEGER, YEAR INTEGER, PRIMARY KEY (ID))
ALTER TABLE ARTICLE ADD CONSTRAINT FK_ARTICLE_VENDOR_ID FOREIGN KEY (VENDOR_ID) REFERENCES VENDOR (ID)
ALTER TABLE ARTICLE ADD CONSTRAINT FK_ARTICLE_VENTE_ID FOREIGN KEY (VENTE_ID) REFERENCES VENTE (ID)
